environment
  tabular_graphics_data

method
      sampling
      sample_type lhs
      seed = 8493
      samples = 4

variables
   active all
   uniform_uncertain = 1 
   lower_bounds = 0.95 
   upper_bounds = 1.05
   descriptors = 'dak_vx'  

interface
   analysis_drivers = 'python dakota_driver.py'
   fork
   work_directory
     named 'rectangle_wrkdir'
   directory_tag
   #directory_save
   copy_files = "rectangle.exo"
                "rectangle.tmpl"
                "dakota_driver.py"
                "rectangle.drive"
                "submission_script.tmpl"
   replace
   parameters_file = 'params.in'
   results_file = 'results.out'
   file_save file_tag
   asynchronous evaluation_concurrency = 1

responses,
        response_functions = 1
        no_gradients
        no_hessians