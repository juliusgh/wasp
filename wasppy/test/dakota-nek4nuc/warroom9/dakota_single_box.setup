
setup_properties {

    runtime_command = "/home/pmc/workbench/rte/entry.sh /home/pmc/workbench/rte/dakota.py"
    application_path = "/projects/dakota/bin/dakota"
    hostname = "warroom9"
    username = "pmc"
    remote_execution_home = "/home/pmc/dakota_stuff"
    application_name = "d_n_wr9"
