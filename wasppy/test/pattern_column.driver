application 'python fake_driver_app.py'
    input_file 'fake_app_input'
    input_tmpl 'my_fake_app.tmpl'
extract_from 'fake_driver_app.csv'
    find pattern "x"
        column 2 delimiter ' ' as 'x1' 
        column 3 delimiter ' ' as 'x2' 
    find pattern "v"
        column 1 delimiter ' ' as 'v1' 
        column 2 delimiter ' ' as 'v2'     
